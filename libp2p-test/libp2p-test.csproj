    <ItemGroup>
      <PackageReference Include="Nethermind.Libp2p" Version="1.0.0-preview.51" />
      <PackageReference Include="Nethermind.Libp2p.Protocols.IpTcp" Version="1.0.0-preview.51" />
    </ItemGroup>

</Project>
